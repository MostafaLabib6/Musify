@using Musify.MVC.Models.ModelViews.SongModelView;
@model List<SongViewModel>;
@{
    ViewData["Title"] = "Home Page";
}
<div>
    @foreach (var song in Model)
    {
        <h1>@song.Name</h1>
        @foreach(var artist in song.Artists)
        {
            <h4>@artist.UserName</h4>
        }
    }

</div>